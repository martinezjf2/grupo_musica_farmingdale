
<!-- <div class="section textInputs"> -->
    <div class="container-fluid newCourseForm">
      <h3 class="welcomeBack" style="text-align: center;"><%= t('new-video-heading') %></h3>



      <%= form_for :video, url: videos_path do |f| %>
      <%= hidden_field_tag :page, params[:page] || 1  %>
        <div class="mb-3">
          <%= f.label :name, t('label-video-name'), class: "form-label" %>
          <%=f.text_field :name, class: "form-control Input", :placeholder => t('place-name-video') %>
        </div>

        <div class="mb-3">
            <%= f.label :link_to_video, t('label-link-video'), class: "form-label" %>
            <%=f.text_field :link_to_video, class: "form-control Input", :placeholder => t('place-url-video') %>
          </div>

        <div class="mb-3">
            <%= f.label :date_created, t('label-date-created'), class: "form-label" %>
            <%=f.date_field :date_created, class: "form-control Input"%>
        </div>

        <div class="mb-3">
          <%= f.label :category, t('label-category'), class: "form-label" %>
          <%= f.select :category, ['Predicas', 'Juvenil', 'Musica', 'Dramas', 'Testimonios', 'Inspiracion', 'Retiro/Vigilia', 'Podcast'], { prompt: t('place-category-video') }, class: "form-control Input" %>
        </div>
      
        <%=f.submit t('new-video-button'), class: "btn btn-lg btn-default loginButton" %> 
      <%end%>


      

    </div>
  <!-- </div> -->
  